{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "globals": {
    "Babel": true,
    "React": true
  },
  "plugins": [
  ],
  "rules": {
    "no-restricted-syntax": 0,
    "jsx-a11y/href-no-hash": 0,
    "semi": 0,
    "comma-dangle": 0,
    "prefer-arrow-callback": 0
  }
}
